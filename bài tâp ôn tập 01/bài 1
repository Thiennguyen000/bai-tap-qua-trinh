ab#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
int main() {
    double a, b, c;
    cin >> a >> b >> c;
    cout << fixed << setprecision(2);
    if (a == 0) {
        if (b == 0) {
            if (c == 0) {
                cout << "Phuong trinh co vo so nghiem" << endl;
            } else {
                cout << "Phuong trinh vo nghiem" << endl;
            }
        } else {
            double x = -c / b;
            cout << "Phuong trinh co 1 nghiem" << endl;
            cout << x << endl;
        }
    } else {
        // TÃ­nh delta
        double delta = b * b - 4 * a * c;
        if (delta > 0) {
            double x1 = (-b + sqrt(delta)) / (2 * a);
            double x2 = (-b - sqrt(delta)) / (2 * a);
            cout << "Phuong trinh co 2 nghiem" << endl;
            if (x1 < x2) {
                cout << x1 << " " << x2 << endl;
            } else {
                cout << x2 << " " << x1 << endl;
            }
        } else if (delta == 0) {
            double x = -b / (2 * a);
            cout << "Phuong trinh co 1 nghiem" << endl;
            cout << x << endl;
        } else {
            cout << "Phuong trinh vo nghiem" << endl;
        }
    }
    
    return 0;
}
